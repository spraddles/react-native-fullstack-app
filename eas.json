{
    "cli": {
        "version": ">= 16.3.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "channel": "development",
            "environment": "preview",
            "distribution": "internal",
            "developmentClient": true,
            "android": {},
            "ios": {
                "resourceClass": "default",
                "simulator": false
            },
            "autoIncrement": true
        },
        "preview": {
            "channel": "preview",
            "environment": "preview",
            "distribution": "store",
            "ios": {
                "resourceClass": "default",
                "simulator": false
            },
            "autoIncrement": true
        },
        "production": {
            "channel": "production",
            "environment": "production",
            "distribution": "store",
            "ios": {
                "resourceClass": "default",
                "simulator": false
            },
            "autoIncrement": true
        }
    }
}