{
  "cli": {
    "version": ">= 16.3.1",
    "appVersionSource": "remote"
    // "requireCommit": true
  },

  "build": {
    "dev": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {},
      "ios": {
        "resourceClass": "default",
        "simulator": true
      },
      "environment": "development", // LOCAL and for testing purposes
      "channel": "dev" // eas OTA updates channel
    },
    "preview": {
      "channel": "preview",
      "distribution": "store",
      "ios": {
        "resourceClass": "default",
        "simulator": false
      },
      "environment": "preview", // Testflight -> preview environment
      // "prebuildCommand": "npx expo prebuild --clean",
      "autoIncrement": true
    },
    "production": {
      "distribution": "store",
      "ios": {
        "resourceClass": "default",
        "simulator": false
      },
      "environment": "production", // App store -> production environment
      "channel": "production"
    }
  }
}
