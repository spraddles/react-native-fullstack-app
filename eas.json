{
  "cli": {
    "version": ">= 16.3.1",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "dev": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {},
      "ios": {
        "resourceClass": "default",
        "simulator": true
      },
      "environment": "development",
      "channel": "dev"
    },
    "preview": {
      "channel": "preview",
      "distribution": "store",
      "ios": {
        "resourceClass": "default",
        "simulator": false
      },
      "environment": "preview",
      // "prebuildCommand": "npx expo prebuild --clean",
      "autoIncrement": true
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "ios": {
        "resourceClass": "default",
        "simulator": false
      },
      "environment": "production"
    }
  }
}
